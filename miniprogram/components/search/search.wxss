.search {
  background: #d43c43;
  color: #fefefe  ;
  font-size: 24rpx;
  margin-left: 20rpx;  
}
.container {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  position: relative;
}
.bar {
  flex: 1;
  background: #f5f5f5;
  border-radius: 50rpx;
  font-size: 28rpx;
  padding-left: 60rpx;
  height: 68rpx;
}
.in-bar {
  color:#999;

}
.find {
  position: absolute;
  top:50%;
  transform: translate(16rpx, -50%);
  color: #9f9f9f;
}